<?xml version="1.0" encoding="utf-8"?>
<VoiceCommands xmlns="http://schemas.microsoft.com/voicecommands/1.1">
  <CommandSet xml:lang="en-us" Name="Lab13CommandSet_en-us">
    <CommandPrefix></CommandPrefix>
    <Example> I'm going to work on Algebra at Lab 1 </Example>

    <Command Name="nuevaPractica">
      <Example> I'm going to work on Algebra at Lab 1  </Example>
      <ListenFor> I'm going to work on {asignatura} at {lugar} </ListenFor>
      <Feedback> Planning the {asignatura} assignment </Feedback>
      <Navigate/>
    </Command>

    <PhraseTopic Label="persona" Scenario="Form Filling">
      <Subject>Person Names</Subject>
    </PhraseTopic>
	
	 <PhraseTopic Label="asignatura" Scenario="Form Filling">
    </PhraseTopic>
	
	 <PhraseTopic Label="fecha" Scenario="Form Filling">
      <Subject>Date/Time</Subject>
    </PhraseTopic>
	
	 <PhraseTopic Label="lugar" Scenario="Form Filling">
    </PhraseTopic>
	
  </CommandSet>

<!-- Other CommandSets for other languages -->

</VoiceCommands>